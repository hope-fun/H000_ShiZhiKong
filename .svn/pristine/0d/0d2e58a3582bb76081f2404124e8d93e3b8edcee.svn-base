.content {
	width: 98%;
	margin: 0 auto;
	padding-top: 75px;
	padding-bottom: 84px;
	font-size: 20pt;
}

.service-bg {
	width: 100%;
	text-align: center;
	background-size: 100% 100%;
	text-align: center;
}

.type1>div {
	width: 90%;
	text-align: left;
	margin: 0 auto;
	padding:12px 0;
	overflow:hidden;
}

.type2>div {
	width: 90%;
