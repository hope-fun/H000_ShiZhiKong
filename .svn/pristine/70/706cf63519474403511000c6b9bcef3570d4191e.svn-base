.contentLightRay #layer_02 img {
	opacity:0;
	-webkit-animation-timing-function: linear;
	-webkit-animation-name: LightRay_Layer_02;
	-webkit-animation-duration: 2000ms;
	-webkit-animation-iteration-count: 1;
}

.contentLightRay #layer_03 img {
	opacity:0;
	-webkit-animation-timing-function: linear;
	-webkit-animation-name: LightRay_Layer_03;
	-webkit-animation-duration: 2000ms;
	-webkit-animation-iteration-count: 1;
}

@-webkit-keyframes LightRay_Layer_02 {
	0% {
		-webkit-transform:matrix(-0.1,0,0,-0.1,0,0);
	}
	10% {
		opacity:1;
	}
	50% {
		-webkit-transform:matrix(30,0,0,30,0,0);
		opacity:1;
	}
	90% {
		opacity:1;
	}
	100% {
		-webkit-transform:matrix(-99,0,0,-99,0,0);
		opacity:0;
	}
}

@-webkit-keyframes LightRay_Layer_03 {
	0% {
		-webkit-transform:matrix(-0.1,0,0,-0.1,0,0);
		opacity:1;
	}
	10% {
		opacity:1;
	}
	50% {
		-webkit-transform:matrix(30,0,0,30,0,0);
		opacity:1;
	}
	90% {
		opacity:1;
	}
	100% {
		-webkit-transform:matrix(-99,0,0,-99,0,0);
		opacity:0;
	}
}
			
