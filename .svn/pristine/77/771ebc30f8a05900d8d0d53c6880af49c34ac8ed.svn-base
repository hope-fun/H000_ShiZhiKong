

BODY>DIV#mask{
    display:block;
    width:100%; 
    /*height:100%;*/ 
    background:#000; 
    z-index: 999999; 
    position:absolute; 
    top:0; 
    left:0;
}

.jqPopup {
    display: block;
    float:left;
    width: 600px;
    height: 350px;
    border: solid 1px #72767b;
    /*-webkit-box-shadow: 0px 4px 6px #555, 0 0 20px rgba(255,255,255,0.5);*/
    -webkit-border-radius: 10px;
    border-radius:10px;
    padding: 10px;
    opacity: 1;
    -webkit-transform: scale(1);
    /*-webkit-transition: all 0.20s  ease-in-out;*/
    transform:scale(1);
    /*transition: all 0.20s  ease-in-out;*/
    position: absolute;
    z-index: 1000000;
    margin-left: auto!important;
    margin-right: auto!important;
    /*background: rgba(70,70,70,1);
    background-image: -webkit-gradient(linear, left top, left bottom, from(rgba(70,70,70,0.8)),    to(rgba(0,20,20,0.8)));*/
    background: url(../images/border/feedback-border.png) no-repeat;
    background-size:100% 100%;
}
.jqPopup >* {
	color:white;
}

.jqPopup.hidden {
    opacity: 0;
    -webkit-transform: scale(0);
    top: 50%;
    left: 50%;
    margin: 0px auto;
}

.jqPopup>.message{
    font-size:24pt;
    margin:50px 8px 8px 8px;
    height:160px;
    text-align:center;
}

.jqPopup>.footer{
    width:100%;    
    text-align:center;
    /*display:block !important;*/
}

.jqPopup>.footer>A{
	color: white;
	width:168px;
    height:53px;
	margin:50px;
	text-decoration: none;	
}

.jqPopup>.footer>A#cancel{
    float:right;
    background: url(../images/buttons/quxiao-blue.png) no-repeat;
}

.jqPopup>.footer>A#action{
    float:left;
    background: url(../images/buttons/queding-green.png) no-repeat;
}

.jqPopup>.footer>A#actiontips{
    float:left;
    background: url(../images/buttons/queding-green.png) no-repeat;
}

.jqPopup>.footer>A.center{
	margin-left:210px;
}