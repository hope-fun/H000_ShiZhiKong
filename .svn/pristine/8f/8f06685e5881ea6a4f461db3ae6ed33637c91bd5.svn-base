/* Processing Bar Beginning*/
.processing-bar {
	position: static;
	display: table;
	width: 100%;
}

.processing-bar>div {
	display: table-cell;
	vertical-align: middle;
	position: static;
	top: 50%;
	width: 100%;
}

.processing-bar>div>div {
	float: left;
	line-height: 33px;
	position: relative;
	top: -50%;
}

.processing-bar>div>div:last-child {
	float: none;
	clear: both;
}

.processing-bar>div>div>span {
	line-height: 33px;
	padding-left: 10px;
}

.processing-bar>div>div:last-child {
	padding-left: 10px;
}

.processing-bar table {
	width: 100%;
	border-collapse: collapse;
}

.processing-bar table td {
	height: 24px;
	padding: 0;
	text-align: left;
	background-size: auto auto;
}

.processing-bar table td>div {
	width: 100%;
	height: 100%;
	padding: 0;
	text-align: left;
}

.processing-bar table td:first-child,.processing-bar table td:last-child
	{
	width: 6px;
}

/** Green color process bar */
.processing-bar-green table td:first-child {
	background: url(../images/process-bar/g01.png) right center no-repeat;
}

.processing-bar-green table td:last-child {
	background: url(../images/process-bar/g02.png) left center no-repeat;
}

.processing-bar-green table td {
	background: url(../images/process-bar/g03.png) center center repeat-x;
}

.processing-bar-green table td>div {
	background: url(../images/process-bar/g04.png) center center repeat-x;
}

.processing-bar-green table td.fill {
	background: url(../images/process-bar/g05.png) right center no-repeat;
}

.processing-bar-green table td.full {
