/** Yellow color process bar */
.processing-bar-yellow table td:first-child {
	background: url(../images/process-bar/y01.png) right center no-repeat;
}

.processing-bar-yellow table td:last-child {
	background: url(../images/process-bar/y02.png) left center no-repeat;
}

.processing-bar-yellow table td {
	background: url(../images/process-bar/y03.png) center center repeat-x;
}

.processing-bar-yellow table td>div {
	background: url(../images/process-bar/y04.png) center center repeat-x;
}

.processing-bar-yellow table td.fill {
	background: url(../images/process-bar/y05.png) right center no-repeat;
}

.processing-bar-yellow table td.full {
	background: url(../images/process-bar/y06.png) left center no-repeat;
}

/** Blue color process bar */



.processing-bar-blue table td {
	background: url(../images/process-bar/b03.png) center center repeat-x;
}

.processing-bar-blue table td>div {
	background: url(../images/process-bar/b04.png) center center repeat-x;
}

.processing-bar-blue table td.fill {
	background: url(../images/process-bar/b05.png) right center no-repeat;
}

.processing-bar-blue table td.full {
	background: url(../images/process-bar/b06.png) left center no-repeat;
}
.processing-bar-blue table td:last-child {
	background: url(../images/process-bar/b02.png) left center no-repeat;
}

.processing-bar-blue table td:first-child {
	background: url(../images/process-bar/b01.png) right center no-repeat;
}
/** Pink color process bar */
.processing-bar-pink table td:first-child {
	background: url(../images/process-bar/p01.png) right center no-repeat;
}

.processing-bar-pink table td:last-child {
	background: url(../images/process-bar/p06.png) left center no-repeat;
}

.processing-bar-pink table td {
	background: url(../images/process-bar/p03.png) center center repeat-x;
}

.processing-bar-pink table td>div {
	background: url(../images/process-bar/p04.png) center center repeat-x;
}

.processing-bar-pink table td.fill {
	background: url(../images/process-bar/p05.png) right center no-repeat;
}

.processing-bar-pink table td.full {
	background: url(../images/process-bar/p02.png) left center no-repeat;
}

