.animationHolder {
	padding-top: 82px;
	width: 720px;
	overflow: hidden;
	background: url(../images/animation/cardBackground.jpg) center center
		no-repeat;
	background-size: 100% 100%;
}

.LightRay {
	width: 100%;
	position: relative;
}

.LightRay>div {
	position: absolute;
	top: 150px;
	left: 0px;
	z-index: 999;
}

.LightRay>div>img {
	width: 100%;
	opacity: 0;
}

.contentLightRay>div>img {
	opacity: 1;
}

.contentLightRay .layer_01 {
	-webkit-animation-timing-function: ease-in-out;
	-webkit-animation-name: LightRay_Layer_01;
	-webkit-animation-duration: 1000ms;
	-webkit-animation-iteration-count: 1;
}

.contentLightRay .layer_02 {
	-webkit-animation-timing-function: ease-in-out;
	-webkit-animation-name: LightRay_Layer_02;
	-webkit-animation-duration: 1000ms;
	-webkit-animation-iteration-count: 1;
}

.contentLightRay .layer_03 {
	-webkit-animation-timing-function: ease-in-out;
	-webkit-animation-name: LightRay_Layer_03;
	-webkit-animation-duration: 1000ms;
	-webkit-animation-iteration-count: 1;
}

@-webkit-keyframes LightRay_Layer_01 {
	0% {
		-webkit-transform:matrix(0.1,0,0,0.1,0,0);
	}
	50% {
		-webkit-transform:matrix(-30,0,0,-30,0,0);
	}
	100% {
		-webkit-transform:matrix(99,0,0,99,0,0);
	}
}

@-webkit-keyframes LightRay_Layer_02 {
	0% {
		-webkit-transform:matrix(-0.1,0,0,-0.1,0,0);
	}
	50% {
		-webkit-transform:matrix(30,0,0,30,0,0);
	}
	100% {
		-webkit-transform:matrix(-99,0,0,-99,0,0);
	}
}

@-webkit-keyframes LightRay_Layer_03 {
	0% {
		-webkit-transform:matrix(-0.1,0,0,-0.1,0,0);
	}
	50% {
		-webkit-transform:matrix(30,0,0,30,0,0);
	}
	100% {
		-webkit-transform:matrix(-99,0,0,-99,0,0);
	}
}
.contentIntensify {
	width: 100%;
}

.contentIntensify .topPart {
	position: relative;
}

.contentIntensify .topPart img {
	position: absolute;
	left: 245px;
	top: 980px;
	height: 100%;
	-webkit-transition: all 0.3s;
	opacity: 0;
}

.contentIntensify .upPart {
	position: relative;
	margin: 20px auto;
}

.contentIntensify .downPart {
	position: relative;
	margin: 20px auto;
}

.contentIntensify .upPart .mat {
	position: absolute;
	top: 0px;
	left: 0px;
	height: 100%;
	-webkit-transition: all 0.2s;
	opacity: 0;
}

.contentIntensify .downPart .mat {
	position: absolute;
	top: 0px;
	left: 600px;
	height: 100%;
	-webkit-transition: all 0.2s;
	opacity: 0;
}

.contentIntensify .hiddenPart {
	display: none;
}

.contentEvolution {
	width: 720px;
	overflow: hidden;
	position: relative;
	background: url(../images/animation/cardBackground.jpg) center center
		no-repeat;
	background-size: 100% 100%;
}

.contentEvolution .main {
	position: absolute;
	left: 0px;
	top: 100px;
	-webkit-transition: all 0.2s;
	opacity: 0;
	z-index: 100;
}

.contentEvolution .mat {
	position: absolute;
	right: 0px;
	top: 650px;
	-webkit-transition: all 0.2s;
	opacity: 0;
}

.contentMask {
	position: absolute;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 100%;
	background: black;
	-webkit-transition: all 0.5s;
	z-index: 9999;
}

.finalRes {
