.content {
	width: 98%;
	margin: 0 auto;
	padding-top: 75px;
	padding-bottom: 84px;
	font-size: 20pt;
}

.rewardtitle{
	width: 80%;
	margin: 0 auto;
	padding-top:6px;
	text-align: center;
}

.tab-view {
	height:53px;
	margin-top:10px;
}

.tab-view > div {
	width:25%;
	float:left;
	height:53px;
}

.tab-view > div:last-child {
	clear:both;
	height:0px;
}

.gray {
    -webkit-filter: grayscale(100%);
    -moz-filter: grayscale(100%);
    -ms-filter: grayscale(100%);
    -o-filter: grayscale(100%);
    filter: grayscale(100%);
    filter: gray;
    
    filter:gray;
	-moz-opacity:.4;
	opacity:0.5;
}

.rewardCount {
	vertical-align: middle;
	line-height: 50px;
	margin: 10px;
	float:left;
}

